platform :ios, '11.4'

def rx_pods
  pod 'RxKeyboard', '~> 2.0.0'
  pod 'RxSwift', '~> 6.5.0'
  pod 'RxCocoa', '~> 6.5.0'
end

target 'E_Learning' do
  use_frameworks!
  # 忽略引入库的所有警告
  inhibit_all_warnings!

  # Rx
  rx_pods

  # 网络框架
  pod 'Moya', '15.0'
  # 检测网络
  pod 'ReachabilitySwift', '5.0'
  # 解析json
  pod 'HandyJSON', '5.0.2'
  # 自动布局
  pod 'SnapKit', '5.6.0'
  # 导航栏
  pod 'SHFullscreenPopGestureSwift', '1.2.1'
  # 键盘
  pod 'IQKeyboardManagerSwift', '6.5.10'
  # 自适应宽高比
  pod 'SwiftyFitsize', '1.4.1'
  # Swift 语法
  pod 'Then', '3.0.0'
  # 加载动画
  pod 'NVActivityIndicatorView', '5.1.1'
  #规范资源文件
  pod 'R.swift', '6.1.0'
  # Swift 工具
  pod 'SwifterSwift', '5.2.0'
  #  数据持久化
  pod 'SwiftyUserDefaults', '~> 5.3.0'
  #  加密
  pod 'CryptoSwift', '1.5.1'
  #打印log
  pod 'XCGLogger', '7.0.1'
  
  
  # oc的库
  # 图片缓存库
  pod 'SDWebImage', '5.13.2'
  # 刷新
  pod 'MJRefresh', '3.7.5'
  # HUD
  pod 'MBProgressHUD', '1.2.0'
  # 页面跳转
  pod "RTRootNavigationController", '~> 0.8.0'
end
